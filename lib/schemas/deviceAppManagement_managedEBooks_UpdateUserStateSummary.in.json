                                                "description": "Last sync date and time.",
                                                "format": "date-time"
                                            },
                                            "installState": {
                                                "anyOf": [
                                                    {
                                                        "title": "installState",
                                                        "enum": [
                                                            "notApplicable",
                                                            "installed",
                                                            "failed",
                                                            "notInstalled",
                                                            "uninstallFailed",
                                                            "unknown"
