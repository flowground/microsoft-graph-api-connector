                                            },
                                            "errorCode": {
                                                "type": "string",
                                                "description": "The error code for install failures.",
                                                "nullable": true
                                            },
                                            "osVersion": {
                                                "type": "string",
                                                "description": "OS Version.",
                                                "nullable": true
                                            },
                                            "osDescription": {
                                                "type": "string",
                                                "description": "OS Description.",
                                                "nullable": true
                                            },
                                            "userName": {
                                                "type": "string",
                                                "description": "Device User Name.",
                                                "nullable": true
                                            }
                                        },
                                        "description": "Contains properties for the installation state for a device."
                                    }
                                ],
                                "example": {
                                    "id": "String (identifier)",
                                    "deviceName": "String",
                                    "deviceId": "String",
                                    "lastSyncDateTime": "DateTimeOffset (timestamp)",
                                    "installState": {
                                        "@odata.type": "microsoft.graph.installState"
                                    },
                                    "errorCode": "String",
                                    "osVersion": "String",
                                    "osDescription": "String",
                                    "userName": "String"
                                }
                            }
