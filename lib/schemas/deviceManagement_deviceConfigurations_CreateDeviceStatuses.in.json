            ],
            "example": {
                "id": "String (identifier)",
                "deviceDisplayName": "String",
                "userName": "String",
                "deviceModel": "String",
                "complianceGracePeriodExpirationDateTime": "DateTimeOffset (timestamp)",
                "status": {
                    "@odata.type": "microsoft.graph.complianceStatus"
                },
                "lastReportedDateTime": "DateTimeOffset (timestamp)",
