                                    "title": "complianceStatus",
                                    "enum": [
                                        "unknown",
                                        "notApplicable",
                                        "compliant",
                                        "remediated",
                                        "nonCompliant",
                                        "error",
                                        "conflict",
                                        "notAssigned"
