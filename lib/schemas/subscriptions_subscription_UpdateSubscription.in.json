{
    "type": "object",
    "properties": {
        "requestBody": {
            "allOf": [
                {
                    "title": "entity",
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        }
                    },
                    "example": {
                        "id": "String (identifier)"
                    }
                },
                {
                    "title": "subscription",
                    "type": "object",
                    "properties": {
                        "resource": {
                            "type": "string"
                        },
                        "changeType": {
                            "type": "string"
                        },
                        "clientState": {
                            "type": "string",
                            "nullable": true
                        },
                        "notificationUrl": {
                            "type": "string"
                        },
                        "expirationDateTime": {
                            "pattern": "^[0-9]{4,}-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9]([.][0-9]{1,12})?(Z|[+-][0-9][0-9]:[0-9][0-9])$",
                            "type": "string",
                            "format": "date-time"
                        },
                        "applicationId": {
                            "type": "string",
                            "nullable": true
                        },
                        "creatorId": {
                            "type": "string",
                            "nullable": true
                        }
                    }
                }
            ],
            "example": {
                "id": "String (identifier)",
                "resource": "String",
                "changeType": "String",
                "clientState": "String",
                "notificationUrl": "String",
                "expirationDateTime": "DateTimeOffset (timestamp)",
                "applicationId": "String",
                "creatorId": "String"
